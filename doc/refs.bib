@article {Li-Chiu-Cox-1999,
	author = "Lu, L. L. and Chiu, S. Y. and Cox, L. A. and Correspondence",
	title = "Optimal project selection: Stochastic knapsack with finite time horizon",
	journal = "Journal of the Operational Research Society",
	volume = "50",
	number = "6",
	year = "1999",
	abstract = "A time-constrained capital-budgeting problem arises when projects, which can contribute to achieving a desired target state before a specified deadline, arrive sequentially. We model such problems by treating projects as randomly arriving requests, each with a funding cost, a proposed benefit, and a known probability of success. The problem is to allocate a non-renewable initial budget to projects over time so as to maximise the expected benefit obtained by a certain time, <I>T</I>, called the deadline, where <I>T</I> can be either a constant or a random variable. Each project must be accepted or rejected as soon as it arrives. We developed a stochastic dynamic programming formulation and solution of this problem, showing that the optimal strategy is to dynamically determine &#096;acceptance intervals&#146; such that a project of type <I>i</I> is accepted when, and only when, it arrives during an acceptance interval for projects of type <I>i</I>.",
	pages = "645-650",
	url = "http://www.ingentaconnect.com/content/pal/01605682/1999/00000050/00000006/2600721"
}


