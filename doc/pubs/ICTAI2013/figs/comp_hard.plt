# Hard data
# Two ways of generating a 2D heat map from ascii data
#
set terminal epslatex standalone color
set output 'comp_hard.tex'
set title "Comparing exact solution with heuristic solution, $\\alpha=0.1$"
unset key
set tic scale 0

# Color runs from white to green
set palette rgbformula 3,3,3
set cbrange [0.994:nan0]
set cblabel "Heuristic solution / Exact solution Ratio"
set cbtics scale 0

set xrange [4.5:15.5]
set yrange [4.5:15.5]

set xtics 5,1,15
set ytics 5,1,15

set xlabel "Number of years"
set ylabel "Number of actions"

set label 1 "$na$" at 12,14 front center
set label 2 "$na$" at 13,12 front center
set label 3 "$na$" at 13,11 front center
set label 4 "$na$" at 13,10 front center
set label 5 "$na$" at 14,15 front center
set label 6 "$na$" at 14,14 front center
set label 7 "$na$" at 14,13 front center
set label 8 "$na$" at 15,15 front center
set label 9 "$na$" at 15,14 front center
set label 10 "$na$" at 15,13 front center
set label 11 "$na$" at 15,12 front center
set label 12 "$na$" at 15,11 front center
set label 13 "$na$" at 15,9 front center
set label 14 "$na$" at 15,8 front center
set label 15 "$na$" at 14,7 front center

set view map
splot '-' using 1:2:3 with image
